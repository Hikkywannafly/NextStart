{
  "name": "nextstart-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "Next.js 16 + Supabase + shadcn/ui + Tailwind CSS v4 boilerplate starter",
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nextstart-boilerplate"
  },
  "keywords": [
    "nextjs",
    "nextjs-16",
    "react",
    "react-19",
    "supabase",
    "tailwindcss",
    "tailwind-v4",
    "shadcn-ui",
    "typescript",
    "boilerplate",
    "starter",
    "template"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "biome lint --write .",
    "lint:fix": "biome lint --write . --unsafe",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "commit": "git cz",
    "prepare": "husky install",
    "build:prepare": "pnpm run typecheck && pnpm run lint && pnpm run format && pnpm run check && pnpm run build",
    "db:types": "supabase gen types typescript --project-id YOUR_PROJECT_ID > src/types/db/database.types.ts"
  },
  "dependencies": {
    "@biomejs/biome": "^2.3.2",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
