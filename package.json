    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "lucide-react": "^0.552.0",
    "motion": "^12.23.24",
    "next": "16.1.0-canary.12",
    "next-intl": "^4.4.0",
    "next-themes": "^0.4.6",
    "nextjs-toploader": "^3.9.17",
    "react": "19.2.0",
