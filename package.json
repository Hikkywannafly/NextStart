    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^20",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "baseline-browser-mapping": "^2.9.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,html,md}": [
      "biome check --write ."
    ]
  }
}
